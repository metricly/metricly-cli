{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "esSpecCompliant": true
            }
        ],
        "max-line-length": [
          true,
          200
        ]
    },
    "rulesDirectory": []
}
